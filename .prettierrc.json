{
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2,
    "arrowParens": "always",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react$",
        "<THIRD_PARTY_MODULES>",
        "^@gamdom",
        "^@admin-",
        "^[./]",
        "^./(.*).module.scss$"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
